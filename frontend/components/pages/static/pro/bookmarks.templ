package pro

import (
	"fdt-templ/components/layouts"
)

templ Bookmarks() {
	@layouts.BaseLayout(layouts.BaseLayoutProps{
		Title:       "My Bookmarks - Free DevTools",
		Description: "View and manage your bookmarked pages",
		Canonical:   "/freedevtools/pro/bookmarks/",
		ShowHeader:  true,
		HideBanner:  true,
	}) {
		<div id="bookmarks-root"></div>
		<script type="module">
			import '/freedevtools/static/js/index.js';
			renderTool('bookmarks', 'bookmarks-root');
		</script>
	}
}

