package t

import (
	"fdt-templ/components/common"
	"fdt-templ/components/layouts"
		"fdt-templ/internal/db/tools"
)

templ MarkdownToHtmlConverter() {
	@layouts.BaseLayout(layouts.BaseLayoutProps{
		Title:        tools.ToolsConfig["markdown-to-html-converter"].Title,
		Description:  tools.ToolsConfig["markdown-to-html-converter"].Description,
		Canonical:    tools.ToolsConfig["markdown-to-html-converter"].Canonical,
		Keywords:     tools.ToolsConfig["markdown-to-html-converter"].Keywords,
		OgImage:      tools.ToolsConfig["markdown-to-html-converter"].OgImage,
		TwitterImage: tools.ToolsConfig["markdown-to-html-converter"].TwitterImage,
		ShowHeader:   true,
		
	}) {
		<div id="markdown-to-html-converter-root"></div>
		<script type="module">
			import '/freedevtools/static/js/index.js';
			renderTool('markdown-to-html-converter', 'markdown-to-html-converter-root');
		</script>
		<div class="max-w-6xl mx-auto px-2 md:px-6 mb-10 mt-12">
			@common.SeeAlso()
		</div>
	}
}
