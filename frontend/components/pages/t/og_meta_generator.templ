package t

import (
	"fdt-templ/components/common"
	"fdt-templ/components/layouts"
		"fdt-templ/internal/db/tools"
)

templ OgMetaGenerator() {
	@layouts.BaseLayout(layouts.BaseLayoutProps{
		Title:        tools.ToolsConfig["og-meta-generator"].Title,
		Description:  tools.ToolsConfig["og-meta-generator"].Description,
		Canonical:    tools.ToolsConfig["og-meta-generator"].Canonical,
		Keywords:     tools.ToolsConfig["og-meta-generator"].Keywords,
		OgImage:      tools.ToolsConfig["og-meta-generator"].OgImage,
		TwitterImage: tools.ToolsConfig["og-meta-generator"].TwitterImage,
		ShowHeader:   true,
		
	}) {
		<div id="og-meta-generator-root"></div>
		<script type="module">
			import '/freedevtools/static/js/index.js';
			renderTool('og-meta-generator', 'og-meta-generator-root');
		</script>
		<div class="max-w-6xl px-2 md:px-6 mb-10 mt-12">
			@common.SeeAlso()
		</div>
	}
}
