package t

import (
	"fdt-templ/components/common"
	"fdt-templ/components/layouts"
		"fdt-templ/internal/db/tools"
)

templ RegexTester() {
	@layouts.BaseLayout(layouts.BaseLayoutProps{
		Title:        tools.ToolsConfig["regex-tester"].Title,
		Description:  tools.ToolsConfig["regex-tester"].Description,
		Canonical:    tools.ToolsConfig["regex-tester"].Canonical,
		Keywords:     tools.ToolsConfig["regex-tester"].Keywords,
		OgImage:      tools.ToolsConfig["regex-tester"].OgImage,
		TwitterImage: tools.ToolsConfig["regex-tester"].TwitterImage,
		ShowHeader:   true,
		
	}) {
		<div id="regex-tester-root"></div>
		<script type="module">
			import '/freedevtools/static/js/index.js';
			renderTool('regex-tester', 'regex-tester-root');
		</script>
		<div class="max-w-6xl px-2 md:px-6 mb-10 mt-12">
			@common.SeeAlso()
		</div>
	}
}
