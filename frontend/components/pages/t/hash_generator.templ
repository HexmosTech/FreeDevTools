package t

import (
	"fdt-templ/components/common"
	"fdt-templ/components/layouts"
		"fdt-templ/internal/db/tools"
)

templ HashGenerator() {
	@layouts.BaseLayout(layouts.BaseLayoutProps{
		Title:        tools.ToolsConfig["hash-generator"].Title,
		Description:  tools.ToolsConfig["hash-generator"].Description,
		Canonical:    tools.ToolsConfig["hash-generator"].Canonical,
		Keywords:     tools.ToolsConfig["hash-generator"].Keywords,
		OgImage:      tools.ToolsConfig["hash-generator"].OgImage,
		TwitterImage: tools.ToolsConfig["hash-generator"].TwitterImage,
		ShowHeader:   true,
		
	}) {
		<div id="hash-generator-root"></div>
		<script type="module">
			import '/freedevtools/static/js/index.js';
			renderTool('hash-generator', 'hash-generator-root');
		</script>
		<div class="max-w-6xl mx-auto px-2 md:px-6 mb-10 mt-12">
			@common.SeeAlso()
		</div>
	}
}
