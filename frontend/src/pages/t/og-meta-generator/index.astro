---
import type { Tool } from '@/config/tools';
import { getToolByKey } from '@/config/tools';
import ToolDetailLayout from '@/components/tool/ToolDetailLayout.astro';
import OgMetaGenerator from './_OgMetaGenerator';

const tool = getToolByKey('og-meta-generator') as Tool;
---

<ToolDetailLayout tool={tool}>
  <OgMetaGenerator client:load />
</ToolDetailLayout>
