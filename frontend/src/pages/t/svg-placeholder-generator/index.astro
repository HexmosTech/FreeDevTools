---
import type { Tool } from '@/config/tools';
import { getToolByKey } from '@/config/tools';
import ToolDetailLayout from '@/components/tool/ToolDetailLayout.astro';
import SvgPlaceholderGenerator from './_SvgPlaceholderGenerator';

const tool = getToolByKey('svg-placeholder-generator') as Tool;
---

<ToolDetailLayout tool={tool}>
  <SvgPlaceholderGenerator client:load />
</ToolDetailLayout>
