{
    "name": "freedevtools",
    "displayName": "Free DevTools",
    "description": "3,50,000+ Free Dev Resources - No Login Required",
    "version": "0.0.3",
    "publisher": "Hexmos",
    "icon": "assets/images/logo.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/HexmosTech/freedevtools.git"
    },
    "homepage": "https://hexmos.com/freedevtools",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "freedevtools.search",
                "title": "Free DevTools: Search"
            }
        ],
        "keybindings": [
            {
                "command": "freedevtools.search",
                "key": "ctrl+alt+f",
                "mac": "cmd+alt+f"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "node esbuild.js --production",
        "dev": "node esbuild.js --watch",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^18.19.130",
        "@types/vscode": "^1.80.0",
        "esbuild": "^0.27.2",
        "eslint": "^8.41.0",
        "typescript": "^5.1.3"
    }
}