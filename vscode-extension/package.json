{
    "name": "fdt-search",
    "displayName": "FDT Search",
    "description": "Search Free Dev Tools",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "fdt.search",
                "title": "FDT: Search"
            }
        ],
        "keybindings": [
            {
                "command": "fdt.search",
                "key": "ctrl+alt+f",
                "mac": "cmd+alt+f"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "18.x",
        "@types/vscode": "^1.80.0",
        "eslint": "^8.41.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@types/cheerio": "^1.0.0",
        "cheerio": "^1.2.0",
        "node-fetch": "^3.3.1"
    }
}